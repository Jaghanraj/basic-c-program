 #include <stdio.h>

int main()
{
 int i;
 int arr[10];
 int sum=0;
 for(i=0;i<=9;i++)
 {
     printf("Enter the %d elements\n",i+1);
     scanf("%d",&arr[i]);
 }
 for(i=0;i<=9;i++)
 {
     sum=sum+arr[i];
 }
 printf("The sum of 10 array element is %d",sum);
 return 0;
}


output

Enter the 1 elements
1
Enter the 2 elements
2
Enter the 3 elements
3
Enter the 4 elements
4
Enter the 5 elements
5
Enter the 6 elements
6
Enter the 7 elements
7
Enter the 8 elements
8
Enter the 9 elements
9
Enter the 10 elements
10
The sum of 10 array element is 55
