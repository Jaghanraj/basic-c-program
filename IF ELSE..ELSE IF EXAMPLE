 
#include <stdio.h>

int main()
{

int a,b,c;
printf("Enter 3 integer values:");
scanf("%d%d%d",&a,&b,&c);
if(a>b && a>c)
{
    printf("A has the highest vslue");
}
else if(b>a && b>c)
{
    printf("B is the highest vslue");
}
else
{
    printf("C is highest value");
    
}
    
 return 0;
 
}


OUTPUT

Enter 3 integer values:10 5 5
A has the highest vslue
