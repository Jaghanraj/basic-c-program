 #include <stdio.h>

int main()
{
 int n,r,sum=0,temp;
 printf("Enter an integer number:");
 scanf("%d",&n);
 temp=n;
 while(n>0)
 {
     r=n%10;
     sum=sum+(r*r*r);
     n=n/10;
 }
 if(temp==sum)
 printf("armstrong numbers");
 else
 printf("not an amstrong");
 return 0;
}

output

Enter an integer number:370
armstrong numbers
